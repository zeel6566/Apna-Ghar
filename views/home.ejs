      </div>
      <div class="col-md">
        <img src="/assets/image4.jpg" style="height: 537px;width: 520px">
      </div>
    </div>
  </section>

  <section class="services">
    <div class="box-inside"
      style="border: 3px solid black;height: 400px; margin: -50px 100px 100px 100px;position: relative;z-index: 1;background-color: white;box-shadow: 5px 10px black">

      <hr style="height: 3px;color: grey;margin: 50px 100px 50px 100px">

      <table style="margin: 0px 100px 0px 100px">
        <tr>
          <td>
            <h2 style="color: black;letter-spacing: 2px; ">BENEFITS OF RENTING WITH US</h2>
          </td>
          <td>
            <img src="assets/affordablerent.jpg" height="200px" width="200px" style="margin: 0px 50px 0px 50px">
            <h4 align="center" style="font-weight: bold;">Affordable Rents</h3>
          </td>
          <td>
            <img src="assets/zerobrokerage.jpg" height="200px" width="200px" style="margin: 0px 50px 0px 50px">
            <h4 align="center" style="font-weight: bold;">Zero Brokerage</h3>
          </td>
          <td>
            <img src="assets/instantdeposit.jpg" height="200px" width="200px" style="margin: 0px 50px 0px 50px">
